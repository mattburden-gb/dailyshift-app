{
  "manifest_version": 3,
  "name": "SnapStash",
  "version": "1.2",
  "description": "Save the current tab to a shopping list by category.",
  "permissions": ["tabs", "storage", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  },
  "icons": {
    "128": "icon128.png"
  }
}


